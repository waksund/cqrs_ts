{
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "projects": {
    "spaghetti": {
      "type": "application",
      "root": "apps/spaghetti",
      "entryFile": "main",
      "sourceRoot": "apps/spaghetti/src",
      "compilerOptions": {
        "tsConfigPath": "apps/spaghetti/tsconfig.spaghetti.json",
        "deleteOutDir": true,
        "webpack": false
      }
    },
    "services-write": {
      "type": "application",
      "root": "apps/services-write",
      "entryFile": "main",
      "sourceRoot": "apps/services-write/src",
      "compilerOptions": {
        "tsConfigPath": "apps/services-write/tsconfig.services-write.json",
        "deleteOutDir": true,
        "webpack": false
      }
    },
    "services-read": {
      "type": "application",
      "root": "apps/services-read",
      "entryFile": "main",
      "sourceRoot": "apps/services-read/src",
      "compilerOptions": {
        "tsConfigPath": "apps/services-read/tsconfig.services-read.json",
        "deleteOutDir": true,
        "webpack": false
      }
    }
  }
}
